<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tiny.learning_rxandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".RxJavaActivity"
            android:screenOrientation="portrait" />

        <activity android:name=".ComposeExampleActivity" />

        <activity android:name=".FlatMapExampleActivity" />

        <activity android:name=".LiftExampleActivity" />

        <activity android:name=".MapExampleActivity" />

        <activity android:name=".SchedulerExampleActivity" />

        <activity android:name=".ThrottleFirstExampleActivity" />

        <activity android:name=".RxBaseExampleActivity" />

        <activity android:name=".UsageExampleActivity" />
    </application>

</manifest>
